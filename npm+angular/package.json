{
  "name": "web-sdk-tutorial-angular",
  "version": "1.0.0",
  "description": "Tiro Web SDK showcase with Angular",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --host 0.0.0.0 --port 3000",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "serve": "ng serve",
    "generate-npmrc": "gcloud artifacts print-settings npm --scope=@tiro-health --repository=npm-ext --location=europe > .npmrc",
    "artifactregistry-login": "npx google-artifactregistry-auth --repo-config=./.npmrc --credential-config=./.npmrc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.3.2",
    "@angular/common": "^20.3.2",
    "@angular/compiler": "^20.3.2",
    "@angular/core": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@tiro-health/web-sdk": "^0.0.21",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.3",
    "@angular/cli": "^20.3.3",
    "@angular/compiler-cli": "^20.3.2",
    "@angular/forms": "^20.3.2",
    "@angular/platform-browser-dynamic": "^20.3.2",
    "@angular/router": "^20.3.2",
    "@tiro-health/web-sdk": "^0.0.21",
    "@types/node": "^24.6.0",
    "@types/react-dom": "^19.1.9",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "zone.js": "^0.15.1"
  }
}
